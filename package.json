{
  "name": "interception",
  "version": "1.0.0",
  "description": "Interceptor library for the native fetch command inspired by fetch-interceptor.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "exit 0",
    "build": "npx tsup ./src/index.ts --format esm,cjs --legacy-output --dts",
    "prepare": "npm run build"
  },
  "homepage": "https://github.com/OmgImAlexis/interception",
  "bugs": {
    "url": "https://github.com/OmgImAlexis/interception/issues"
  },
  "repository": {
    "url": "https://github.com/OmgImAlexis/interception.git"
  },
  "keywords": [
    "fetch",
    "request",
    "intercept",
    "interceptor",
    "hooks"
  ],
  "author": "Alexis Tyler <xo@wvvw.me>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^3.15.0",
    "cross-fetch": "^3.0.6",
    "ts-node": "^9.1.1",
    "tsup": "^3.11.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist"
  ],
  "ava": {
    "files": [
      "test/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
